<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://www.ibatis.com/dtd/sql-map-2.dtd">

<sqlMap namespace="Cctv">
		
	<select id="CctvDAO.selectCctvInfo" parameterClass="string" resultClass="string" >
		SELECT
			hls_url
		FROM
			tbl_cctv_info
		WHERE
			cctv_id = #cctvId#
	</select>
</sqlMap>